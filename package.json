{
  "name": "vscode-starlims",
  "displayName": "Starlims VS Code Extension",
  "description": "Unofficial dictionary explorer (and more) for STARLIMS.",
  "license": "SEE LICENSE IN LICENSE.md",
  "version": "1.2.6",
  "publisher": "Marius Popovici, Christoph DÃ¶llinger",
  "keywords": [
    "STARLIMS",
    "SSL",
    "STARLIMS Scripting Language"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mariuspopovici/starlimsvscode"
  },
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other",
    "SCM Providers",
    "Programming Languages"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "STARLIMSVSCode",
          "title": "STARLIMS",
          "icon": "resources/starlims.png",
          "alwaysShow": true
        }
      ]
    },
    "views": {
      "STARLIMSVSCode": [
        {
          "id": "STARLIMS",
          "name": "STARLIMS Enterprise"
        }
      ]
    },
    "commands": [
      {
        "command": "STARLIMS.Refresh",
        "title": "Refresh Explorer"
      },
      {
        "command": "STARLIMS.Checkout",
        "title": "Check Out"
      },
      {
        "command": "STARLIMS.Checkin",
        "title": "Check In"
      },
      {
        "command": "STARLIMS.Connect",
        "title": "Connect to STARLIMS"
      },
      {
        "command": "STARLIMS.Compare",
        "title": "Compare with Remote Version"
      },
      {
        "command": "STARLIMS.RunScript",
        "title": "Run Script"
      },
      {
        "command": "STARLIMS.Save",
        "title": "Save File"
      },
      {
        "command": "STARLIMS.ClearLog",
        "title": "Clear Log"
      },
      {
        "command": "STARLIMS.selectEnterpriseItem",
        "title": "Refresh Log"
      },
      {
        "command": "STARLIMS.Search",
        "title": "Search for items...",
        "icon": {
          "dark": "resources/dark/search.svg",
          "light": "resources/light/search.svg"
        }
      },
      {
        "command": "STARLIMS.ShowTreeView",
        "title": "Show/Refresh Tree View",
        "icon": {
          "dark": "resources/dark/treeview.svg",
          "light": "resources/light/treeview.svg"
        }
      },
      {
        "command": "STARLIMS.OpenForm",
        "title": "Open Form"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "STARLIMS.ShowTreeView",
          "when": "view == STARLIMS",
          "group": "navigation"
        },
        {
          "command": "STARLIMS.Search",
          "when": "view == STARLIMS",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "STARLIMS.Refresh",
          "when": "view == STARLIMS"
        },
        {
          "command": "STARLIMS.RunScript",
          "when": "view == STARLIMS && (viewItem == SS || view == STARLIMS && viewItem == DS || viewItem == APPSS || viewItem == APPDS)"
        },
        {
          "command": "STARLIMS.Checkout",
          "when": "view == STARLIMS && viewItem != SERVERLOG"
        },
        {
          "command": "STARLIMS.Checkin",
          "when": "view == STARLIMS && viewItem != SERVERLOG"
        },
        {
          "command": "STARLIMS.selectEnterpriseItem",
          "when": "view == STARLIMS && viewItem == SERVERLOG"
        },
        {
          "command": "STARLIMS.OpenForm",
          "when": "view == STARLIMS && viewItem == HTMLFORMXML"
        }
      ],
      "explorer/context": [
        {
          "command": "STARLIMS.Compare",
          "when": "view != STARLIMS"
        },
        {
          "command": "STARLIMS.Checkout",
          "when": "view == STARLIMS"
        },
        {
          "command": "STARLIMS.Checkin",
          "when": "view == STARLIMS"
        }
      ],
      "editor/context": [
        {
          "command": "STARLIMS.Compare",
          "when": "resourceDirname =~ /SLVSCODE/ && !editorReadonly && editorLangId != 'log'"
        },
        {
          "command": "STARLIMS.RunScript",
          "when": "resourceDirname =~ /SLVSCODE/ && editorLangId == 'SSL'"
        },
        {
          "command": "STARLIMS.Save",
          "when": "resourceDirname =~ /SLVSCODE/ && editorLangId != 'log'"
        },
        {
          "command": "STARLIMS.selectEnterpriseItem",
          "when": "resourceDirname =~ /SLVSCODE/ && !editorReadonly && editorLangId == 'log'"
        },
        {
          "command": "STARLIMS.ClearLog",
          "when": "resourceDirname =~ /SLVSCODE/ && !editorReadonly && editorLangId == 'log'"
        }
      ]
    },
    "configuration": [
      {
        "type": "object",
        "title": "STARLIMS VSCode Configuration",
        "properties": {
          "STARLIMS.url": {
            "type": "string",
            "default": "",
            "description": "STARLIMS Installation URL. For example: https://server/STARLIMS.Dev/"
          },
          "STARLIMS.user": {
            "type": "string",
            "default": "",
            "description": "STARLIMS User ID"
          },
          "STARLIMS.password": {
            "type": "string",
            "default": "",
            "description": "STARLIMS User Password"
          },
          "STARLIMS.rootPath": {
            "type": "string",
            "default": "",
            "description": "Root path for temporary STARLIMS files. For example: C:\\STARLIMS\\. SLVSCODE will be appended to this path."
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "npm version patch && webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/mocha": "^9.1.0",
    "@types/node": "^14.18.12",
    "@types/node-fetch": "^2.6.4",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "ts-loader": "^9.2.8",
    "typescript": "^4.5.5",
    "webpack": "^5.84.1",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "jsdom": "^22.1.0",
    "node-fetch": "^2.6.7"
  }
}
